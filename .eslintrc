{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-shadow": 0,
    "no-console": 0,
    "func-names": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "max-len": [2, 120, 2],
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
  }
}